@model List<WebNhaHangOnline.Models.Thanhviennhom>
@{
    ViewBag.Title = "Thông tin các thanh viên trong nhóm xây dựng trang web";
    Layout = "~/Views/Shared/_LayoutFrontEnd.cshtml";
}

<div class="container panel panel-primary">
    <h3>Thông tin các thành viên trong nhóm :</h3>
    <hr />
    <table class="table table-bordered">
        <tr class="">
            <th id="MSSV">Mã Sinh Viên</th>
            <th id="hovaten">Họ và Tên</th>
            <th id="link">Link Facebook</th>
        </tr>
        @for (int i = 0; i < Model.Count; i++)
        {
            <tr>
                <td>@Model[i].MSSV</td>
                <td>@Model[i].Hoten</td>
                <td>@Model[i].LinkFacebook</td>
            </tr>
        }
    </table>
</div>

